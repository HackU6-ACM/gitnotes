{
  "manifest_version": 2,

  "name": "GitNotes",
  "description": "This extension adds documentation to a github project",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "js": ["test.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
	"https://www.google.com/*"
  ]
}

